public class OopsStart {
	
	int i = 4;
	int k;
	
	public static void main(String[] args) {
		
		int j = 5;
		
		OopsStart o1 = new OopsStart();
		
		o1.k = o1.i + j;
		
		System.out.println(o1.k);
		

	}

}


